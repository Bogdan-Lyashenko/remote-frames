{
  "name": "@klarna/unicorn-remote-frames",
  "version": "0.0.2",
  "description": "Render a subset of the React tree to a different location, from many locations, without having to coordinate them",
  "main": "dist/index.js",
  "scripts": {
    "build": "sagui build",
    "dist": "sagui dist",
    "format": "sagui format",
    "prepublishOnly": "babel src/ -d dist/ --ignore spec.js,example/",
    "start": "sagui develop --port 3000",
    "test": "sagui test",
    "test:lint": "sagui test:lint",
    "test:typecheck": "sagui test:typecheck",
    "test:unit": "sagui test:unit --coverage",
    "test:unit:watch": "sagui test:unit --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/klarna/unicorn-remote-frames.git"
  },
  "keywords": [],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/klarna/unicorn-remote-frames/issues"
  },
  "homepage": "https://github.com/klarna/unicorn-remote-frames#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "prop-types": "^15.6.0",
    "ramda": "^0.24.1",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "react-redux": "^5.0.6",
    "recompose": "^0.25.1",
    "redux": "^3.7.2",
    "sagui": "^11.1.0"
  },
  "peerDependencies": {
    "prop-types": "^15.6.0",
    "ramda": "^0.24.1",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "recompose": "^0.25.1"
  }
